{
  "name": "parallel-dev",
  "version": "0.1.0",
  "description": "Run multiple AI coding agents in parallel with isolated branches and native terminals",
  "private": true,
  "license": "MIT",
  "author": "Optiviselab",
  "homepage": "https://paralleldev.optiviselab.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/optivise-lab/parallel-dev.git"
  },
  "bugs": {
    "url": "https://github.com/optivise-lab/parallel-dev/issues"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "postinstall": "electron-rebuild -f -w node-pty"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "simple-git": "^3.27.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "node-pty": "^1.0.0",
    "lucide-react": "^0.460.0"
  },
  "build": {
    "appId": "com.paralleldev.app",
    "productName": "ParallelDev",
    "mac": {
      "target": "dmg",
      "icon": "public/favicon.png",
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^6.0.3",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
